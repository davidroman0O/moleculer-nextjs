{
  "name": "moleculer-nextjs",
  "version": "0.0.10",
  "description": "Mixin for Node-nextjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "test": "jest --coverage",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer",
    "nextjs"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:davidroman0O/moleculer-nextjs.git"
  },
  "homepage": "https://github.com/davidroman0O/moleculer-nextjs#readme",
  "author": "David Roman",
  "peerDependencies": {
    "moleculer": "^0.11.0"
  },
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "3.0.0",
    "eslint": "4.8.0",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "lolex": "^2.1.3",
    "moleculer": "0.11.1",
    "moleculer-docgen": "0.2.1",
    "nodemon": "1.12.1",
    "npm-check": "5.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "dependencies": {
    "babel-plugin-transform-define": "^1.3.0",
    "express": "^4.16.3",
    "get-routes": "^0.2.0",
    "ncp": "^2.0.0",
    "next": "^6.0.4-canary.3",
    "path": "^0.12.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^4.6.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.1"
  }
}
